<?xml version="1.0" encoding="UTF-8"?>

<!-- @@author limcel -->
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox fx:id="stackPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="name" styleClass="cell_big_label" text="\$name">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../images/student(32px).png"/>
                    </image>
                </ImageView>
            </graphic>
        </Label>
        <Label fx:id="phone" styleClass="cell_small_label" text="\$phone">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../images/phone.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Label>
        <Label fx:id="address" styleClass="cell_small_label" text="\$address">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../images/address.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Label>
        <Label fx:id="formClass" styleClass="cell_small_label" text="\$formClass">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../images/class.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Label>
        <Label fx:id="grades" styleClass="cell_small_label" text="\$grades">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../images/grades.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Label>
        <Label fx:id="postalCode" styleClass="cell_small_label" text="\$postalCode">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../images/postalCode.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Label>
        <Label fx:id="email" styleClass="cell_small_label" text="\$email">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../images/email.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Label>
        <Label fx:id="remark" styleClass="cell_small_label" text="\$remark">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../images/remark.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Label>
    </children>
</VBox>
