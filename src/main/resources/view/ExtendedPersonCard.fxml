<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="stackPane" alignment="TOP_LEFT" minHeight="105" spacing="15.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
      <padding>
          <Insets bottom="5" left="5" right="15" top="5" />
      </padding>
      <HBox alignment="TOP_LEFT" spacing="10">
          <Label fx:id="name" styleClass="cell_big_label" text="\$first">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <graphic>
            <ImageView>
               <image>
                  <Image url="@../../../../docs/images/icons/student(32px).png" />
               </image>
            </ImageView>
         </graphic>
      </Label>
      </HBox>
      <Label fx:id="phone" styleClass="cell_small_label" text="\$phone">
      <graphic>
         <ImageView>
            <image>
               <Image url="@../../../../docs/images/icons/phone.png" />
            </image>
         </ImageView>
      </graphic>
      <VBox.margin>
         <Insets />
      </VBox.margin>
   </Label>
      <Label fx:id="address" styleClass="cell_small_label" text="\$address">
      <graphic>
         <ImageView>
            <image>
               <Image url="@../../../../docs/images/icons/address.png" />
            </image>
         </ImageView>
      </graphic>
   </Label>
      <Label fx:id="formClass" styleClass="cell_small_label" text="\$formClass">
      <graphic>
         <ImageView>
            <image>
               <Image url="@../../../../docs/images/icons/class.png" />
            </image>
         </ImageView>
      </graphic>
   </Label>
      <Label fx:id="grades" styleClass="cell_small_label" text="\$grades">
      <graphic>
         <ImageView>
            <image>
               <Image url="@../../../../docs/images/icons/grades.png" />
            </image>
         </ImageView>
      </graphic>
   </Label>
      <Label fx:id="postalCode" styleClass="cell_small_label" text="\$postalCode">
          <graphic>
              <ImageView>
                  <image>
                      <Image url="@../../../../docs/images/icons/postalCode.png" />
                  </image>
              </ImageView>
          </graphic>
   </Label>
      <Label fx:id="email" styleClass="cell_small_label" text="\$email">
      <graphic>
         <ImageView>
            <image>
               <Image url="@../../../../docs/images/icons/email.png" />
            </image>
         </ImageView>
      </graphic>
   </Label>
      <Label fx:id="remark" styleClass="cell_small_label" text="\$remark">
      <graphic>
         <ImageView>
            <image>
               <Image url="@../../../../docs/images/icons/remark.png" />
            </image>
         </ImageView>
      </graphic>
   </Label>
  </VBox>
